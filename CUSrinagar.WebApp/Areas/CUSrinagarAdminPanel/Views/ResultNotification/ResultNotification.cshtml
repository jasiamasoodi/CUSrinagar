@{
    ViewBag.Title = "CUS :: Reports";
}
@section CUSSitemap{
    <div class="col-md-5">
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="@Url.Action("Index","Dashboard",new { area="CUSrinagarAdminPanel"})">Home</a>
            </li>
            <li class="active">Result List</li>
        </ul><!-- /.breadcrumb -->
    </div>
    <div class="col-md-7">
        @*<h2 class="no-margins">
                Result Reports
            </h2>*@
    </div>
    <div class="col-md-4">
    </div>
}

<div class="row">
    <div class="col-md-12 jsTableContent">
        <div class="panel panel-primary js-pager-table" data-otherparam1="@CUSrinagar.Enums.PrintProgramme.UG">
            <div class="panel-heading">
                <h3 class="panel-title">
                    Result
                    <small>
                        <i class="ace-icon fa fa-angle-double-right"></i>
                        List of Student
                    </small>
                    <span class="clickable filter-btn pull-right" data-toggle="tooltip" title="Toggle table filter" data-container="body">
                        <a href="#"><i class="fa fa-search" aria-hidden="true"></i>&nbsp;Search</a>
                    </span>
                </h3>
            </div>
            <div class="panel-body">
                <div class="search-box">
                    <form class="form-inline" action="@Url.Action("SubjectWise","Result")" id="Result-notification-list-admin">
                        <div class="form-group">
                            <label>Batch</label><br />
                            <div class="jsDDLContainer">
                                @Html.DropDownList("BatchDDL", new SelectList(ViewBag.Batchs, "Value", "Text"),
                                      new
                                      {
                                          @class = "form-control chosen-select jsfilterelement",
                                          @data_column = "Batch",
                                          @data_operator = "EqualTo",
                                          @data_scope = "row1"
                                      })
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Programme</label><br />
                            <div class="form-group">
                                @Html.DropDownList("ProgrammeDDL", new SelectList(ViewBag.Programmes, "Value", "Text"), " -- All --",
                                           new
                                           {
                                               @class = "form-control chosen-select jsfilterelement",
                                               @data_column = "PrintProgramme",
                                               @data_operator = "EqualTo",
                                               @data_scope = "row1"
                                           })
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Choose Semester</label><br />
                            <div class="jsDDLContainer">
                                <select class="jsfilterelement chosen-select form-control" data-column="Semester" data-operator="EqualTo" id="SemesterDDL" data-scope="row1">
                                    <option value=""> -- All --</option>
                                    <option value="1">Semester-I</option>
                                    <option value="2">Semester-II</option>
                                    <option value="3">Semester-III</option>
                                    <option value="4">Semester-IV</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Status</label><br />
                            <div class="jsDDLContainer">
                                <select class="jsfilterelement chosen-select form-control" data-column="ImportedToMasterTable" data-operator="EqualTo" id="" data-scope="row1">
                                    <option value="">All</option>
                                    <option value="0" selected="selected">Not Imported</option>
                                    <option value="1">Imported</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="invisible">for styling purpose</label><br />
                            <button type="button" class="btn btn-primary btn-sm fa-search-btn js-search"><i class="fa fa-search"></i>&nbsp;Search</button>
                            <div class="btn-group">
                                <button data-toggle="dropdown" class="btn btn-primary btn-sm dropdown-toggle">
                                    Action
                                    <span class="ace-icon fa fa-caret-down icon-on-right"></span>
                                </button>
                                <ul class="dropdown-menu dropdown-default dropdown-info">
                                    <li>
                                        <a href="javascript:void(0)" class="js-exportToCSV" data-url="@Url.Action("CSV","ResultNotification",new { area="CUSrinagarAdminPanel"})" data-otherparam1="printProgramme=@CUSrinagar.Enums.PrintProgramme.UG">
                                            Download Excel(CSV)
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="js-table-content" data-url=@Url.Action("ResultNotificationListPartial") data-DefaultOrderByColumn="Dated">
            </div>
            @{Html.RenderPartial("_JSPagerTableFooter");}
        </div>
    </div>
</div>


@section PageSpecificJS{
    <script src="@Url.Content("~/Content/ThemeAdmin/Content/Scripts/Pages/reports-result.js")"></script>

}
