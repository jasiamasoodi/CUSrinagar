@{
    ViewBag.Title = "CUS :: PendingList";
}
@section CUSSitemap{
    <div class="col-md-5">
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="@Url.Action("Index","Dashboard",new { area="CUSrinagarAdminPanel"})">Home</a>
            </li>
            <li class="active">PendingList</li>
        </ul>
    </div>
    <div class="col-md-7">
    </div>
    <div class="col-md-4">
    </div>
}

<div class="row">
    <div class="col-md-12 jsTableContent">    
            <div class="panel panel-primary js-pager-table" data-otherparam1="@CUSrinagar.Enums.PrintProgramme.UG">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        Pending List
                        <small>
                            <i class="ace-icon fa fa-angle-double-right"></i>
                            Result Pending List
                        </small>
                    </h3>
                </div>
                <div class="panel-body">

                    <form class="form-inline">

                        <div class="form-group">
                            <label>Exam-Form-Submitted-Year</label><br />
                            <div class="jsDDLContainer ">
                                <select class="jsfilterelement form-control" data-column="Batch" data-operator="EqualTo" id="BatchDDL" data-tablealias="P" data-table style="width:186px;">
                                    <option value="@DateTime.Now.Year">@DateTime.Now.Year</option>
                                    <option value="@DateTime.Now.AddYears(-1).Year">@DateTime.Now.AddYears(-1).Year</option>
                                </select>
                            </div>
                        </div>



                        <div class="form-group">
                            <label>Programme</label><br />
                            <div class="wd-140 jsDDLContainer">
                                @Html.DropDownList("ProgrammeDDL", new SelectList(ViewBag.Programmes, "Value", "Text", (short)CUSrinagar.Enums.PrintProgramme.UG), null,
                            new
                            {
                                @class = "form-control chosen-select jsProgrammeDDL jsfilterelement",
                                @data_column = "Programme",
                                @data_operator = "EqualTo",
                                @data_tablealias = "CMs",
                                @data_scope = "row1"
                            })
                            </div>
                        </div>

                        <div class="form-group">
                            <label>Choose Semester</label><br />
                            <div class="jsDDLContainer">
                                @Html.DropDownList("Semester", new SelectList(ViewBag.Semesters, "Value", "Text", 1), "--All--",
                   new
                   {
                       @class = "jsfilterelement",
                       @data_column = "Semester",
                       @data_operator = "EqualTo",
                       @data_tablealias = "CM"
                   })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="invisible">for styling purpose</label><br />
                            <div class="btn-group">
                                <div class="form-group">
                                    <button type="button" class="btn btn-primary btn-sm fa-search-btn js-search"><i class="fa fa-search"></i>&nbsp;Search</button>
                                </div>
                            </div>
                        </div>
                    </form>

                </div>
                <div class="js-table-content" data-url=@Url.Action("ResultBacklogBatchesPartial") data-DefaultOrderByColumn="ExamRollNumber">
                </div>
                @{Html.RenderPartial("_JSPagerTableFooter");}
            </div>
        </div>
</div>



<style>

    .filter-btn a {
        color: white !important
    }
</style>
