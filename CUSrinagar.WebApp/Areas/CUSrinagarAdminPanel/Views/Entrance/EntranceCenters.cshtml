@model CUSrinagar.Models.SemesterCenters

@{
    ViewBag.Title = "Entrance Centers ";
    bool IsAllowed = HttpContext.Current.User.IsInRole(CUSrinagar.Enums.AppRoles.University.ToString());
}
<div id="Jspleasewait" style="position:fixed;background-color:rgba(0, 0, 0, 0.70);left:0;top:0;z-index:200;width:100%;height:100%">
    <p style="font-size:30px;font-weight:bold;color:white;margin-top:14%;text-align:center">
        Working on it...
    </p>
</div>

<div class="page-wrapper pager-table-text">
    <div class="page-header">
        <div style="padding-bottom: 30px;">
            <div class="col-sm-12">
                <div class="col-sm-3">
                    <h1 class="heading-title pull-left">
                        Entrance Centers
                        <small>
                            <i class="ace-icon fa fa-angle-double-right"></i>
                            List
                        </small>
                    </h1>
                </div>
                <div class="col-sm-9 align-right">
                    @if (IsAllowed)
                    {
                        <button type="button" id="JSDeleteDataFromCenterAllotment" href="javascript:void(0);" class="btn btn-sm btn-danger"><i class="fa fa-trash"></i>&nbsp;Delete all data from CenterAllotment</button>
                        <span style="margin-left:1%"></span>
                        <a type="button" href="/CUSrinagarAdminPanel/Entrance/AdmitCardStatus" class="btn btn-sm btn-success"><i class="fa fa-user"></i>&nbsp;Assign RollNo &amp; Center to Individual</a>
                    }
                    <div class="btn-group btn-sm">
                        <button data-toggle="dropdown" class="btn btn-primary dropdown-toggle" aria-expanded="false">
                            Download Details As Excel
                            <span class="ace-icon fa fa-caret-down icon-on-right"></span>
                        </button>
                        <ul class="dropdown-menu dropdown-default dropdown-info">
                            <li>
                                <a type="button" href="#ModalCourseWiseExamFormCount" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Course Wise Admission Form Count</a>
                            </li>
                            @if (IsAllowed)
                            {
                                <li>
                                    <a type="button" href="#ModalCenterWiseCourseCount" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Center Wise Courses Count</a>
                                </li>
                                <li>
                                    <a type="button" href="#ModalDateSheetFile" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Download Date Sheet</a>
                                </li>
                                <li>
                                    <a type="button" href="#ModalUploadEntranceResult" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Upload Entrance Result</a>
                                </li>
                            }
                            <li>
                                <a type="button" href="#ModalOMRMasterFile" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Download OMR Master File</a>
                            </li>
                            <li>
                                <a type="button" href="#ModalAttendanceSheetFile" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Download Attendance Sheets</a>
                            </li>
                            <li>
                                <a type="button" href="#ModalGeneralMeritList" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Download General Merit List</a>
                            </li>
                            <li>
                                <a type="button" href="#ModalCategoryMeritList" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Download Category Merit List</a>
                            </li>
                            <li>
                                <a type="button" href="#ModalPDFMeritList" data-toggle="modal"><i class="fa fa-file-pdf-o"></i>&nbsp;Download PDF Merit Lists</a>
                            </li>
                            <li>
                                <a type="button" href="#ModalSelfFinancedList" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Download Self-Financed Lists</a>
                            </li>
                            <li>
                                <a type="button" href="#ModalDistrictWiseCount" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;Download District Wise Form Count</a>
                            </li>
                            <li>
                                <a type="button" href="#ModalDistrictWiseCenterAllotmentCount" data-toggle="modal"><i class="fa fa-file-excel-o"></i>&nbsp;District Center allotment Count</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="response" style="width:80%;margin:0 auto">
    @if (TempData["response"] != null)
    {
        @Html.Raw(TempData["response"])
    }
</div>
<div class="page-content">
    <div class="row page-row">
        <div class="col-sm-12 center">
            @if (IsAllowed)
            {
                <button type="button" href="#ModalCreateCenters" data-toggle="modal" class="btn btn-sm btn-purple"><b>Step-1</b> <i class="fa fa-home"></i>&nbsp;Create Center(s)</button>
                <span style="margin-right:1%"></span>
                <button type="button" href="#ModalAssignRollNosInBulk" data-toggle="modal" class="btn btn-sm btn-purple"><b>Step-2</b>  <i class="fa fa-home"></i>&nbsp;Assign RollNo's in Bulk</button>
                <span style="margin-right:1%"></span>
                <a type="button" href="/CUSrinagarAdminPanel/Entrance/AssignCentersInBulk" class="btn btn-sm btn-purple"><b>Step-3</b> <i class="fa fa-home"></i>&nbsp;Assign Centers in Bulk</a>
                <span style="margin-right:1%"></span>
            }
            <button type="button" href="#ModalDownloadCenterNotice" data-toggle="modal" class="btn btn-sm btn-purple"><b>Step-4</b> <i class="fa fa-file-pdf-o"></i>&nbsp;Download Center Notices</button>
        </div>

        <div class="col-lg-12 margin-bottom-10"></div>
        <div class="content-wrapper col-md-10 col-sm-8 jsSingleContent"></div>

        <div class="col-md-12 jsTableContent">
            <div class="panel panel-primary js-pager-table">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <i class="fa fa-home" aria-hidden="true"></i> Active Centers
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="col-sm-12 search-box">
                        <div style="color:#2679B5" class="col-sm-3 lbl bigger-120 no-padding-right">
                            <i class="fa fa-filter"></i> Filter by Centers *
                        </div>
                        <div class="col-sm-5 lbl bigger-120 no-padding-right">
                            @Html.DropDownList("SearchCollege_ID", (IEnumerable<SelectListItem>)ViewBag.Colleges, new { @class = "form-control" })
                        </div>
                    </div>
                </div>
                <div class="JSContent" style="overflow:auto !important">
                    @{Html.RenderPartial("_DisplayCenters", (List<CUSrinagar.Models.ARGEntranceCentersMaster>)ViewBag.Data);}
                </div>
            </div>
        </div>
    </div>
</div>

@*------------------ Download Center Notice model -------------------------*@
<div id="ModalDownloadCenterNotice" class="modal fade" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h3 class="smaller lighter no-margin"><i class="fa fa-file-pdf-o"></i>&nbsp;Center Notice</h3>
            </div>

            <div class="modal-body JSCreateNoticeContent">
                <div class="msg2"></div>
                @using (Html.BeginForm("DownloadCenterNotices", "Entrance", FormMethod.Post, new { id = "frmCreateNoticeContent" }))
                {
                    @Html.AntiForgeryToken();
                    <div class="form-group">
                        <div class="col-sm-12">
                            <label class="control-label no-padding-right">
                                @Html.LabelFor(sc => sc.PrintCenterNotice.College_ID) <span class="red">*</span>
                            </label>
                            <span class="red">@Html.ValidationMessageFor(sc => sc.PrintCenterNotice.College_ID)</span>
                            @Html.DropDownListFor(sc => sc.PrintCenterNotice.College_ID, (IEnumerable<SelectListItem>)ViewBag.Colleges, string.Empty, new { @class = "form-control" })
                        </div>
                        <br />
                        <br />
                        <div class="col-sm-12">
                            <label class=" control-label no-padding-right">
                                @Html.LabelFor(sc => sc.PrintCenterNotice.CourseCategory) <span class="red">*</span>
                            </label>
                            <span class="red">@Html.ValidationMessageFor(sc => sc.PrintCenterNotice.CourseCategory)</span>
                            @Html.DropDownListFor(sc => sc.PrintCenterNotice.CourseCategory, (IEnumerable<SelectListItem>)ViewBag.Programmes, string.Empty, new { @class = "form-control" })
                        </div>
                        <br />
                        <br />
                        <div class="col-sm-12">
                            <label class=" control-label no-padding-right">
                                Batch <span class="red">*</span>
                            </label>
                            <span class="red">@Html.ValidationMessageFor(sc => sc.PrintCenterNotice.ExaminationYear)</span>
                            @Html.TextBoxFor(sc => sc.PrintCenterNotice.ExaminationYear, new { @maxlength = "4", @type = "number", @class = "form-control" })
                        </div>
                        <br />
                        <br />
                        @Html.Hidden("PrintCenterNotice.Semester", "1")
                        <div class="col-sm-12"></div>
                        <br />
                        <br />
                        <div class="col-sm-12 text-center">
                            <button type="submit" class="btn btn-sm btn-success JSDCNotice">
                                <i class="ace-icon fa fa-check"></i>
                                Download Center Notice
                            </button>
                        </div>
                        <br />
                    </div>
                }
            </div>

            <div class="modal-footer">
                <button class="btn btn-sm btn-danger pull-right" data-dismiss="modal">
                    <i class="ace-icon fa fa-times"></i>
                    Close
                </button>
            </div>
        </div>
    </div>
</div>


