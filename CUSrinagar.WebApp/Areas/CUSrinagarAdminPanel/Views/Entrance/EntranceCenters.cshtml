                            <span class="red">@Html.ValidationMessageFor(sc => sc.PrintCenterNotice.ExaminationYear)</span>
                            @Html.TextBoxFor(sc => sc.PrintCenterNotice.ExaminationYear, new { @class = "form-control", @maxlength = "4" })
                        </div>
                        <br />
                        <br />
                        <div class="col-sm-12">
                            <label class=" control-label no-padding-right">
                                Select Course <span class="red">*</span>
                            </label>
                            <span class="red">@Html.ValidationMessageFor(sc => sc.College_ID)</span>
                            <span class="CourseID">
                                @Html.DropDownListFor(sc => sc.College_ID, (IEnumerable<SelectListItem>)ViewBag.Courses, string.Empty, new { @class = "form-control" })
                            </span>
                        </div>
                        @Html.Hidden("PrintCenterNotice.Semester", "1")
                        <br />
                        <br />
                        <br />
                        <div class="col-sm-12"></div>
                        <br />
                        <br />
                        <div class="col-sm-12 text-center">
                            <button type="submit" class="btn btn-sm btn-success JSBtnDWCAllotmentCount">
                                <i class="fa fa-file-excel-o"></i>&nbsp;Download As Excel
                            </button>
                        </div>
                        <br />
                    </div>
                }
            </div>

            <div class="modal-footer">
                <button class="btn btn-sm btn-danger pull-right" data-dismiss="modal">
                    <i class="ace-icon fa fa-times"></i>
                    Close
                </button>
            </div>
        </div>
    </div>
</div>



