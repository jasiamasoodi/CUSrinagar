
@section CUSSitemap{
    <div class="col-md-5">
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="@Url.Action("Index","Dashboard",new { area="CUSrinagarAdminPanel"})">Home</a>
            </li>
            <li class="active">Revaluation Result List</li>
        </ul><!-- /.breadcrumb -->
    </div>
    <div class="col-md-7">
        @*<h2 class="no-margins">
                Result Reports
            </h2>*@
    </div>
    <div class="col-md-4">
    </div>
}

<div class="row">
    <div class="col-md-12 jsTableContent">
        <div class="panel panel-primary js-pager-table js-rapid-entry" data-otherparam1="@CUSrinagar.Enums.PrintProgramme.UG">
            <div class="panel-heading">
                <h3 class="panel-title">
                    Result
                    <small>
                        <i class="ace-icon fa fa-angle-double-right"></i>
                        Update Carefully
                    </small>
                    <span class="clickable filter-btn pull-right" data-toggle="tooltip" title="Toggle table filter" data-container="body">
                        <a href="#"><i class="fa fa-search" aria-hidden="true"></i>&nbsp;Search</a>
                    </span>
                </h3>
            </div>
            <div class="panel-body">
                <div class="search-box">
                    <form class="form-inline">
                        <div class="form-group">
                            <label>Batch</label><br />
                            <div class="jsDDLContainer">
                                <select class="jsfilterelement form-control jsBatchDDL" data-column="SemesterBatch" data-operator="EqualTo" id="BatchDDL">
                                    <option value="" selected="selected">--All--</option>
                                    @for (int i = 2017; i <= DateTime.Now.Year; i++)
                                    {
                                        <option value="@i">@i</option>
                                    }
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Choose College</label><br />
                            <div class="jsDDLContainer wd-150">
                                @Html.DropDownList("CollegeDDL", new SelectList(ViewBag.College, "Value", "Text"), "--All--",
                               new
                               {
                                   @class = "chosen-select jsfilterelement jsCollegeDDL",
                                   @data_column = "AcceptCollege_ID",
                                   @data_operator = "EqualTo",
                                   @data_tablealias = "STDINFO",
                                   @data_scope = "row1"
                               })
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Programme</label><br />
                            <div class="wd-140 jsDDLContainer">
                                @Html.DropDownList("ProgrammeDDL", new SelectList(ViewBag.Programmes, "Value", "Text", (short)CUSrinagar.Enums.Programme.UG), null,
                                           new
                                           {
                                               @class = "form-control chosen-select jsProgrammeDDL jsfilterelement",
                                               @data_column = "Programme",
                                               @data_operator = "EqualTo",
                                               @data_tablealias = "crm",
                                               @data_scope = "row1"
                                           })
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Choose Course</label><br />
                            <div class="jsDDLContainer  wd-200">
                                @Html.DropDownList("Course", new SelectList(ViewBag.Courses, "Value", "Text", (short)CUSrinagar.Enums.Programme.UG), " -- All Courses --",
                                           new
                                           {
                                               @class = "form-control chosen-select jsCourseDDL jsfilterelement",
                                               @data_column = "Course_ID",
                                               @data_operator = "EqualTo",
                                               @data_tablealias = "crm",
                                               @data_defaulttextvalue = " -- All Courses --,",
                                               @data_scope = "row1"
                                           })
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Choose Semester</label><br />
                            <div class="jsDDLContainer">
                                @Html.DropDownList("Semester", new SelectList(ViewBag.Semesters, "Value", "Text"),
                                          new
                                          {
                                              @class = "form-control chosen-select jsSemesterDDL jsfilterelement",
                                              @data_column = "Semester",
                                              @data_operator = "EqualTo",
                                              @data_tablealias = "EXM",
                                              @data_scope = "row1"
                                          })
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Choose Subjects</label><br />
                            <div class="jsDDLContainer wd-200">
                                <select required id="SubjectDDL" data-tablealias="R" class="jsfilterelement chosen-select form-control jsVWSCSubjectDDL" data-column="Subject_ID" data-operator="EqualTo" data-defaulttextvalue="All Subjects," data-scope="row1">
                                    <option value="" selected>All Subjects</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label>Date From</label><br />
                            <div class="input-group">
                                <input class="form-control jsfilterelement" type="date" data-column="CreatedOn" data-operator="GreaterThanEqualTo" data-tablealias="REV" data_scope = "row1" >
                            </div>
                        </div>
               

                        <div class="form-group">
                            <label>
                                <select class="jsChangeFilterColumn" data-scope="row1">
                                    <option value="ClassRollNo" selected="selected">Search by ClassRollNo</option>
                                    <option value="CUSRegistrationNo">Search by RegistrationNo</option>
                                    <option value="ExamRollNumber">Search by ExamRollNumber</option>
                                </select>
                            </label>
                            <br />
                            <input type="text" value="" data-tablealias="" placeholder="ClassRollNo" name="SearchRegNo"
                                   class="form-control jsfilterelement" data-column="ClassRollNo" data-operator="EqualTo" data-scope="row1" />
                        </div>
                        <div class="form-group">
                            <label class="invisible">for styling purpose</label><br />
                            <button type="button" class="btn btn-primary btn-sm fa-search-btn js-search"><i class="fa fa-search"></i>&nbsp;Search</button>
                            <div class="btn-group">
                                <button data-toggle="dropdown" class="btn btn-primary btn-sm dropdown-toggle">
                                    Action
                                    <span class="ace-icon fa fa-caret-down icon-on-right"></span>
                                </button>
                                <ul class="dropdown-menu dropdown-default dropdown-info">
                                    <li>
                                        <a href="javascript:void(0)" class="js-exportToCSV" data-url="@Url.Action("RapidEntryListCSV","Result",new { area="CUSrinagarAdminPanel"})" data-otherparam1="printProgramme=@CUSrinagar.Enums.PrintProgramme.UG">
                                            Download Excel(CSV)
                                        </a>
                                    </li>
                                    <li class="divider"></li>

                                </ul>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <form class="jsMainForm" name="RevaluationEntryForm" style="font-size:12px;">
                <div class="js-table-content" data-url=@Url.Action("UpdateRevaluationtResultPartial", "Revaluation", new { area = "CUSrinagarAdminPanel" }) data-DefaultOrderByColumn="CUSRegistrationNo">
                </div>
            </form>
            @{Html.RenderPartial("_JSPagerTableFooter");}
        </div>
    </div>
</div>
@*MODAL DIALOG END*@



@section PageSpecificJS{
    <script src="~/Content/ThemeAdmin/Content/Scripts/Pages/revaluation-result.js"></script>
    <script>
        $(document).ready(function () {
            var $pagertable = $(".js-pager-table");
            loadpagertable();
        });
    </script>

    <script>
        $(document).ready(function () {          
            $('.date-picker').datepicker({
                autoclose: true,
                todayHighlight: true,
                dateFormat: 'mm/dd/yyyy',
                format: 'mm/dd/yyyy'
            })
           
        });
    </script>
}