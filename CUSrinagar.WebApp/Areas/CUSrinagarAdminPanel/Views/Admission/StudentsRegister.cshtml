@{
    ViewBag.Title = "Register Of Students - CUSrinagar";
    string currentYear = DateTime.Now.Year + "";
}

@section PageSpecificStyle{
    <style type="text/css">
        .required {
            color: black;
        }

        .filter-btn a {
            color: white !important
        }
    </style>
}
<div class="page-wrapper pager-table-text">
    <div class="page-content">
        <div class="row page-row">
            <div class="content-wrapper col-md-10 col-sm-8 jsSingleContent"></div>
            <div class="col-md-12 jsTableContent">
                <div class="panel panel-primary js-pager-table">
                    <div class="panel-heading">
                        <h3 class="panel-title">
                            Register Of Students (Passout only)
                            <span class="filter-btn pull-right" data-toggle="tooltip" title="Toggle table filter" data-container="body">
                                <a href="#"><i class="fa fa-search" aria-hidden="true"></i>&nbsp;Search</a>
                            </span>
                        </h3>
                    </div>
                    <div class="panel-body">
                        <div class="col-sm-12 search-box">
                            <div class="col-sm-2 lbl bigger-120 no-padding-right">
                                <span style="color:red">*</span>College<span id="" class="label label-sm label-danger arrowed-in-right arrowed"></span>
                                <div>
                                    @Html.DropDownList("College_ID", new SelectList(ViewBag.Colleges, "Value", "Text"),
                                          "--Select--", new { @class = "form-control  jsfilterelement required ", @data_column = "College_ID", @data_operator = "EqualTo", @data_ddltype = "" })
                                </div>
                            </div>
                            <div class="col-sm-1 lbl bigger-120 no-padding-right">
                                <span style="color:red">*</span>Batch<span id="YearValidator" class="label label-sm label-danger arrowed-in-right arrowed"></span>

                                <div>
                                    @Html.TextBox("Year", DateTime.Now.Year, new { @type = "number", @min = "2017", @max = "9999", @class = " required form-control jsfilterelement", @data_column = "Year", @data_operator = "EqualTo" })
                                </div>
                            </div>
                            <div class="col-sm-2 lbl bigger-120 no-padding-right">
                                <span style="color:red">*</span>Programme<span id="ProgrammeIdValidator" class="label label-sm label-danger arrowed-in-right arrowed"></span>
                                <div>
                                    @Html.DropDownList("ProgrammeId", new SelectList(ViewBag.ProgrammeList, "Value", "Text"),
                                      "--Select--", new { @class = "form-control SearchDDL  required", @data_column = "ProgrammeId", @data_operator = "EqualTo", data_ddltype = "Course", @data_ddlchildtype = "Semester" })
                                </div>
                            </div>
                            <div class="col-sm-2 lbl bigger-120 no-padding-right">
                                <span style="color:red">*</span>Course<span id="CourseIDValidator" class="label label-sm label-danger arrowed-in-right arrowed"></span>
                                <div id="Course">
                                    @Html.DropDownList("CourseID", new SelectList(ViewBag.CourseList, "Value", "Text"),
                                           "--Select--", new { @class = "form-control  SearchDDL jsfilterelement required ", @data_column = "CourseID", @data_operator = "EqualTo", @data_ddltype = "Semester" })
                                </div>
                            </div>
                            <div class="col-sm-4 no-padding-right">
                                <div class="form-group">
                                    <label class="control-label no-padding-right">
                                        Check for Lateral Entry only <span class="red"></span>
                                    </label>
                                    <input style="height:20px" type="checkbox" id="IsLateralEntry" name="IsLateralEntry" class="form-control jsfilterelement " data-column="IsLateralEntry" data-operator="EqualTo" />
                                </div>
                            </div>
                            <div class="col-sm-12 center" style="padding-top: 17px;">
                                <a href="button" class="btn btn-primary JSDownloadBtn"><i class="fa fa-eye"></i>&nbsp;View Register Of Students</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section PageSpecificJS{
    <script type="text/javascript">
        var chilDDLPath = '@Url.Action("_GetRRDDL", "Admission", new {area = "CUSrinagarAdminPanel" })';
         var gridPathWithParam= '@Url.Action("StudentsRegisterPreview", "Admission", new {area = "CUSrinagarAdminPanel" })';
         var controllerName = "Admission";
    </script>

    <script src="@Url.Content("~/Scripts/JSChosen.js")"></script>
    <script src="@Url.Content("~/Content/ThemeAdmin/Content/Scripts/JSRR/StdRegisterReportUniversity.js")"></script>
}





