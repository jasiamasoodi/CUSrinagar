
@model List<CUSrinagar.Models.MSTranscriptDegreeSettings>
@{
    Layout = null;
}
<table class="table table-hover table-bordered js-table">

    <thead>
        <tr>
            <th>Course</th>
            <th>Batch From</th>
            <th>Batch To</th>
            <th>Semester From</th>
            <th>Semester To</th>
            <th>Total Credits</th>
            <th>Awarded Degree Title</th>
            <th>Program Title</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            if (Model.Count() > 0)
            {
                foreach (var item in Model)
                {
        <tr>
            <td>@item.CourseFullName</td>
            <td>@item.BatchFrom</td>
            <td>@item.BatchTo</td>
            <td>@item.SemesterFrom</td>
            <td>@item.SemesterTo</td>
            <td>@item.TotalCredits</td>
            <td>@item.AwardedDegreeTitle</td>
            <td>@item.ProgramTitle</td>
            <td>@Html.ActionLink("Edit", "EditTranscriptDegreeSettings", new { Setting_ID = item.Setting_ID })</td>
        </tr>
                }
            }
            else
            {
                <tr><td colspan="10">No Record Found</td></tr>
            }
        }
        else
        {
            <tr><td colspan="10">No Record Found</td></tr>
        }
    </tbody>
</table>
