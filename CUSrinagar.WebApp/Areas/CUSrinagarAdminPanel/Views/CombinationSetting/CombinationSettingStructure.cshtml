@{
    ViewBag.Title = "Combination Setting";
}
@section CUSSitemap{
    <div class="col-md-5">
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="@Url.Action("Index","Dashboard",new { area="CUSrinagarAdminPanel"})">Home</a>
            </li>
            <li class="active">Combination Setting Structure</li>
        </ul><!-- /.breadcrumb -->
    </div>
    @*<div class="col-md-7">
            <h2 class="no-margins">
                Combination Setting
            </h2>
        </div>*@
    <div class="col-md-4">
    </div>
}

<div class="row">
    <div class="col-md-12 jsTableContent">
        <div class="panel panel-primary js-pager-table" data-otherparam1="">
            <div class="panel-heading">
                <h3 class="panel-title">
                    Combination Setting Structure
                    <small>
                        <i class="ace-icon fa fa-angle-double-right"></i>
                        List
                    </small>
                    <span class="clickable filter-btn pull-right" data-toggle="tooltip" title="Toggle table filter" data-container="body">
                        <a href="#"><i class="fa fa-search" aria-hidden="true"></i>&nbsp;Search</a>
                    </span>
                </h3>
            </div>
            <div class="panel-body">
                <div class="search-box">
                    <div class="form-inline">

                        <div class="form-group">
                            <label>Remark</label><br />
                            <input type="text" placeholder="Remark" name="Remark" class="form-control jsfilterelement" data-column="Remark" data-operator="Contains" data-scope="row1" />
                        </div>

                        <div class="form-group">
                            <label class="invisible">for styling purpose</label><br />
                            <button type="button" class="btn btn-sm btn-primary fa-search-btn js-search"><i class="fa fa-search"></i>&nbsp;Search</button>
                            <div class="btn-group">
                                <button data-toggle="dropdown" class="btn btn-primary dropdown-toggle">
                                    Action
                                    <span class="ace-icon fa fa-caret-down icon-on-right"></span>
                                </button>
                                <ul class="dropdown-menu dropdown-default dropdown-info">
                                    <li>
                                        <a href="javascript:void(0)" class="js-exportToCSV hidden" data-url="@Url.Action("","",new { area="CUSrinagarAdminPanel"})" data-otherparam1="printProgramme=@CUSrinagar.Enums.PrintProgramme.UG">
                                            Download Excel(CSV)
                                        </a>
                                    </li>
                                    <li>
                                        <a href="javascript:void(0)" class="js-exportToCSV" data-url="@Url.Action("","",new { area="CUSrinagarAdminPanel"})" data-otherparam1="printProgramme=@CUSrinagar.Enums.PrintProgramme.UG">
                                            Print
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="form-group pull-right">
                            <label class="invisible">for styling purpose</label><br />
                            <a class="btn btn-sm- btn-primary" href="@Url.Action("CombinationSettingStructureCreateEdit","CombinationSetting")">Create</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="js-table-content" data-url=@Url.Action("CombinationSettingStructureList", "CombinationSetting", new { area = "CUSrinagarAdminPanel" }) data-DefaultOrderByColumn="CombinationSettingStructure_ID">
            </div>
            @{Html.RenderPartial("_JSPagerTableFooter");}
        </div>
    </div>
</div>


@section PageSpecificJS{
    <script>
        $(document).ready(function () {
            loadpagertableWithDefaultParams();
        });
    </script>
}
