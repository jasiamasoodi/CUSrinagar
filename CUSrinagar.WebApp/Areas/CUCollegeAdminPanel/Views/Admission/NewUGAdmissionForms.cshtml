                            <input class="jsfilterelement" type="hidden" data-column="College_ID" data-operator="EqualTo" value="@CUSrinagar.Models.AppUserHelper.College_ID">
                            <label>Date From</label><br />
                            <div class="input-group">
                                <input class="form-control jsfilterelement" id="id-date-picker-from" type="date" placeholder="To" data-column="TxnDate" data-operator="GreaterThanEqualTo">
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Date To</label><br />
                            <div class="input-group">
                                <input class="form-control jsfilterelement" id="id-date-picker-to" type="date" placeholder="To" data-column="TxnDate" data-operator="LessThanEqualTo">
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Form Number</label><br />
                            <div class="input-group">
                                <input class="form-control jsfilterelement" type="text" placeholder="CUS Form Number" data-column="StudentFormNo" data-operator="EqualTo">
                            </div>
                        </div>
                        <div class="form-group">
