                                                        @foreach (var cat in listOfCategories)
                                                        {
                                                            var catStudents = _failedStudents.Where(x => x.Category == cat);

                                                            <th>
                                                                <center>
                                                                    <b><u>@catStudents.Count()</u></b>
                                                                    <br />
                                                                    [<span title="Male">@catStudents.Where(x => x.Gender.ToLower() == "male").Count()</span><sup>M</sup>
                                                                    |
                                                                    <span title="Female">@catStudents.Where(x => x.Gender.ToLower() == "female").Count()</span><sup>F</sup>]
                                                                </center>
                                                            </th>
                                                        }
                                                    </tr>
                                                }
                                            }
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

