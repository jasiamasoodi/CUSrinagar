@using CUSrinagar.Extensions
@using CUSrinagar.Enums
@using CUSrinagar.Models
@model List<CUSrinagar.Models.ResultCompact>

@if (Model.IsNotNullOrEmpty())
{
    ViewBag.Title = "Result Statistics";
    string StatisticsType = ViewBag.StatisticsType;

    var batches = Model.Select(x => x.Batch).Distinct().OrderBy(x => x).ToList();

    var listOfCourses = Model.Select(x => new { x.Programme, x.CourseFullName })
        .OrderBy(x => x.CourseFullName).OrderBy(x => x.Programme).Select(x => x.CourseFullName).Distinct().ToList();

    var listOfProgrammes = Model.Select(x => x.Programme).Distinct().OrderBy(x => x).ToList();

    var listOfColleges = Model.Select(x => x.AcceptCollege_ID).Distinct().ToList();

    var maxSemesters = Model.SelectMany(x => x.SubjectResults).Max(x => x.Semester);

    var listOfCategories = Model.Select(x => x.Category).Distinct().OrderBy(x => x).ToList();
    var count = 1;

    string semesters = string.Join(" , ", Model?.SelectMany(x => x.SubjectResults)
                        ?.Select(x => x.Semester)?.ToList()?.OrderBy(x => x)
                        ?.DistinctBy(x => x) ?? new List<short>());

    string programmeToDisplay = listOfProgrammes?.First().GetEnumDescription() ?? "";
    string displayCourses = string.Join(" , ", listOfCourses ?? new List<string>());
    string displaySubjects = "ALL";

    if (ViewBag.subjectIDSelected != Guid.Empty.ToString())
    {
        displaySubjects = string.Join(" , ", Model?.SelectMany(x => x.SubjectResults)
            ?.Where(x => x.Subject_ID == Guid.Parse(ViewBag.subjectIDSelected))
            ?.Select(x => $"{x.SubjectFullName}({x.SubjectType.GetEnumDescription()})")
            ?.ToList()?.DistinctBy(x => x) ?? new List<string>());
    }
    string displayFileTitle = $"({programmeToDisplay} Batch {batches.First()} {displayCourses} Semester: {semesters} subjects: {displaySubjects})";

    <div class="page-content">
        <div id="accordion" class="accordion-style1 panel-group">
            <input type="hidden" id="displayFileName" value="@(displayFileTitle.Replace(":","_"))-CUS" />
            @if (StatisticsType == "CourseWise")
            {
                <div class="panel panel-default">
                    <div class="panel-heading hidden-print">
                        <h4 class="panel-title">
                            <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapse1">
                                <i class="ace-icon fa fa-angle-down bigger-110" data-icon-hide="ace-icon fa fa-angle-down" data-icon-show="ace-icon fa fa-angle-right"></i>
                                &nbsp;Statisitics: Batch Wise
                            </a>
                        </h4>
                    </div>

                    <div class="panel-collapse collapse" id="collapse1" style="page-break-after: always; font-size: 10px;">
                        <div class="panel-body">
                            <table class="table table-bordered table-hover jsPrintTabContent">
                                <thead>
                                    <tr>
                                        <th colspan="99">
                                            <b>Statisitics: Batch Wise <br />@displayFileTitle</b>
                                            <a class="pull-right jsPrintTable" style="cursor:pointer;"><i class="fa fa-print"></i></a>
                                        </th>
                                    </tr>
                                    <tr>
                                        <th>S.No</th>
                                        <th>Batch</th>
                                        <th>
                                            <center>
                                                <b><u>Total</u> </b>
                                                <br />
                                                [M|F]
                                            </center>
                                        </th>
                                        <th>
                                            <center>
                                                <b><u>Status</u> </b>
                                            </center>
                                        </th>

                                        <th>
                                            <center>
                                                Result
                                            </center>
                                        </th>
                                        @foreach (var cat in listOfCategories)
                                        {
                                            <th>
                                                <center>
                                                    <b><u>@cat</u> </b>
                                                    <br />
                                                    [M|F]
                                                </center>
                                            </th>
                                        }
                                    </tr>
                                </thead>
                                <tbody>
                                    @{
                                        count = 1;
                                        foreach (short batch in batches)
                                        {
                                            var _listOfStudents = Model.Where(x => x.Batch == batch).ToList();

                                            if (_listOfStudents.IsNotNullOrEmpty())
                                            {
                                                var _passedStudents = _listOfStudents.Where(x => x.HasTranscript ||
                                                (x.SubjectResults.All(y => y.OverallResultStatus == ResultStatus.P && y.ShowPublic)
                                                && x.SubjectResults.Max(y => y.Semester) == maxSemesters)).ToList();
                                                var _failedStudents = _listOfStudents.Where(x => x.Transcript == null && x.SubjectResults.Any(y => y.OverallResultStatus != ResultStatus.P || !y.ResultNotification_ID.HasValue || !y.ExamForm_ID.HasValue) || x.SubjectResults.Max(y => y.Semester) != maxSemesters).ToList();

                                                var discrepencyList = _passedStudents.Where(x => _failedStudents.Where(y => y.Student_ID == x.Student_ID) != null);

                                                var innerJoin = from p in _passedStudents // outer sequence
                                                                join f in _failedStudents //inner sequence
                                                                on p.Student_ID equals f.Student_ID // key selector
                                                                select new
                                                                { // result selector
                                                                    CUSRegistrationNo = p.CUSRegistrationNo,
                                                                    Student_ID = p.Student_ID
                                                                };
                                                <tr>
                                                    <td rowspan="2">@(count++)</td>
                                                    <td rowspan="2">
                                                        @batch
                                                        <h6>@(string.Join(",", innerJoin.Select(x => x.CUSRegistrationNo)))</h6>
                                                    </td>
                                                    <td rowspan="2">
                                                        <center>
                                                            <b><u>@_listOfStudents.Select(x => x.Student_ID).Distinct().Count()</u> </b>
                                                            <br />
                                                            [<span title="Male">@_listOfStudents.Where(x => x.Gender.ToLower() == "male").Count()</span><sup>M</sup>
                                                            |
                                                            <span title="Female">@_listOfStudents.Where(x => x.Gender.ToLower() == "female").Count()</span><sup>F</sup>]
                                                        </center>
                                                    </td>
                                                    <td>
                                                        <b>Pass</b>
                                                    </td>
                                                    <td>
                                                        <center>
                                                            <b><u>@_passedStudents.Select(x => x.Student_ID).Distinct().Count()</u></b>
                                                            <br />
                                                            [<span title="Male">@_passedStudents.Where(x => x.Gender.ToLower() == "male").Count()</span><sup>M</sup>
                                                            |
                                                            <span title="Female">@_passedStudents.Where(x => x.Gender.ToLower() == "female").Count()</span><sup>F</sup>]
                                                        </center>
                                                    </td>
                                                    @foreach (var cat in listOfCategories)
                                                    {
                                                        var catStudents = _passedStudents.Where(x => x.Category == cat);
                                                        <th>
                                                            <center>
                                                                <b><u>@catStudents.Count()</u></b>
                                                                <br />
                                                                [<span title="Male">@catStudents.Where(x => x.Gender.ToLower() == "male").Count()</span><sup>M</sup>
                                                                |
                                                                <span title="Female">@catStudents.Where(x => x.Gender.ToLower() == "female").Count()</span><sup>F</sup>]
                                                            </center>
                                                        </th>
                                                    }
                                                </tr>
                                                <tr>

                                                    <td>
                                                        <b>Fail</b>
                                                    </td>
                                                    <td>
                                                        <center>
                                                            <b><u>@_failedStudents.Count()</u></b>
                                                            <br />
                                                            [<span title="Male">@_failedStudents.Where(x => x.Gender.ToLower() == "male").Count()</span><sup>M</sup>
                                                            |
                                                            <span title="Female">@_failedStudents.Where(x => x.Gender.ToLower() == "female").Count()</span><sup>F</sup>]
                                                        </center>
                                                    </td>

                                                    @foreach (var cat in listOfCategories)
                                                    {
                                                        var catStudents = _failedStudents.Where(x => x.Category == cat);

                                                        <th>
                                                            <center>
                                                                <b><u>@catStudents.Count()</u></b>
                                                                <br />
                                                                [<span title="Male">@catStudents.Where(x => x.Gender.ToLower() == "male").Count()</span><sup>M</sup>
                                                                |
                                                                <span title="Female">@catStudents.Where(x => x.Gender.ToLower() == "female").Count()</span><sup>F</sup>]
                                                            </center>
                                                        </th>
                                                    }
                                                </tr>

                                            }
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

