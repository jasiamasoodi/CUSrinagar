            </a>
        </h1>
        <div class="info col-md-8 col-sm-8">
            <ul class="menu-top navbar-right">
                @{
                    HttpCookie httpCookie = Request.Cookies[CUSrinagar.Models.AuthCookie.Name];
                }
                @if (httpCookie != null)
                {
                    <li class="divider"><a href="@Html.Action("GetControlPanelURL","Dashboard",new { area="CUSrinagarAdminPanel"})"><i class="fa fa-dashboard"></i> Back To Dashboard </a></li>
                    <li class=""><a href="@Url.Action("SignOut", "Account", new { area = string.Empty })"><i class="fa fa-sign-out"></i> SignOut </a></li>
