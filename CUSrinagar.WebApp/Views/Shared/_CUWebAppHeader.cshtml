                {
                    <li class="divider"><a href="@Url.Action("StudentZone", "Account", new { area = string.Empty })"><i class="fa fa-graduation-cap"></i> <strong>Student SignIn</strong></a></li>
                    <li class=""><a href="@Url.Action("SignIn", "Account", new { area = string.Empty })"><i class="fa fa-university"></i> <strong>University SignIn</strong></a></li>
                }
                @*<li class="divider"><a href='@Url.Action("ContactUs","Home",new { area = string.Empty})'>Contact</a></li>*@
                <li class="divider">
                    @*<li><a>FAQ</a></li>*@
